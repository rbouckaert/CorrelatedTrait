<package name='TreeSetAnalyser' version='0.0.5'>
	<depends on='BEAST.base' atleast='2.7.4'/>
	<depends on='BEAST.app' atleast='2.7.4'/>
	<depends on='BEASTLabs' atleast='2.0.0'/>
	<depends on='BEAST_CLASSIC' atleast='1.6.1'/>
	<depends on='NS' atleast='1.1.0'/>
	<depends on='Babel' atleast='0.4.1'/>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="tsa.app.beauti.BeautiCompoundTraitProvider"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CompoundAlignment"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CompoundAlignment4"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CompoundDataType"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CompoundTreeLikelihood"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CorrelatedSubstitutionModel"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.CorrelatedSubstitutionModel2"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.IndependencyLogger"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.IndependentEvolutionPrior"/>
        <provider classname="tsa.correlatedcharacters.polycharacter.RateMatrixVisualiser"/>
        <provider classname="tsa.evolution.alignment.AlignmentFromTraitForDPLace"/>
        <provider classname="tsa.evolution.alignment.DPlaceCSVtoXML"/>
        <provider classname="tsa.evolution.likelihood.IndexedTreeSetLikelihood"/>
        <provider classname="tsa.evolution.likelihood.ThreadedTreeSetLikelihood"/>
        <provider classname="tsa.evolution.likelihood.TreeSetLikelihood"/>
        <provider classname="tsa.evolution.operators.RatesFreqsOperator"/>
        <provider classname="tsa.evolution.operators.UniformGibbsOperator"/>
        <provider classname="tsa.evolution.substitutionmodel.NoChangeSubstitutionModel"/>
        <provider classname="tsa.evolution.substitutionmodel.StationaryFrequenciesLogger"/>
        <provider classname="tsa.evolution.tree.IndexedTreeFromSet"/>
        <provider classname="tsa.parameterclone.helpers.RescaledDirichlet"/>
        <provider classname="tsa.parameterclone.selector.Selector"/>
        <provider classname="tsa.parameterclone.selector.SelectorSet"/>
        <provider classname="tsa.parameterclone.splitandmerge.MergeOperator"/>
        <provider classname="tsa.parameterclone.splitandmerge.SplitOperator"/>
        <provider classname="tsa.tools.TreeFossiliser"/>
        <provider classname="tsa.TSASummary"/>
    </service>

    <service type="beast.base.evolution.datatype.DataType">
        <provider classname="tsa.correlatedcharacters.polycharacter.CompoundDataType"/>
    </service>

    <service type="beastfx.app.inputeditor.InputEditor">
        <provider classname="tsa.app.beauti.CompoundTraitInputEditor"/>
    </service>


	<packageapp description="TSASummary"
              class="tsa.TSASummary"
              args=""
            />
	<packageapp description="CorrelatedRateMatrixVisualiser"
		              class="tsa.correlatedcharacters.polycharacter.RateMatrixVisualiser"
		              args=""
		            />
</package>
